<html>
  <head>
  </head>

  <body>
    <%= form_for @photo, html: {multipart: true} do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.file_field :image %>
      <%= f.label :description %>
      <%= f.text_field :description %>
      <%= f.submit %>
    <% end %>
  </body>
</html>
